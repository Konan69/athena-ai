{
  "name": "@athena-ai/server",
  "version": "1.0.0",
  "main": "src/server.ts",
  "types": "./src/types/index.ts",
  "imports": {
    "#types/*": "./src/types/*"
  },
  "exports": {
    ".": "./src/server.ts",
    "./trpc": "./src/trpc/index.ts",
    "./types": {
      "types": "./src/types/index.ts",
      "default": "./src/types/index.ts"
    },
    "./types/agents": {
      "types": "./src/types/agents.ts",
      "default": "./src/types/agents.ts"
    }
  },
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:typecheck": "tsc --noEmit",
    "test:db:up": "docker compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker compose -f docker-compose.test.yml down",
    "test:db:reset": "docker compose -f docker-compose.test.yml down -v && docker compose -f docker-compose.test.yml up -d",
    "test:db:migrate": "drizzle-kit migrate --config=drizzle.test.config.ts",
    "test:db:generate": "drizzle-kit generate --config=drizzle.test.config.ts",
    "test:db:push": "drizzle-kit push --config=drizzle.test.config.ts",
    "test:setup": "bun run test:db:up && sleep 3 && bun run test:db:push",
    "test:full": "bun run test:setup && bun run test",
    "dev": "bun run --hot src/server.ts  ",
    "db:studio": "bunx drizzle-kit studio",
    "build": "tsc",
    "start": "bun dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "packageManager": "pnpm@10.0.0",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@hono/node-server": "^1.17.1",
    "@hono/trpc-server": "^0.4.0",
    "@hono/zod-validator": "^0.4.3",
    "@mastra/client-js": "^0.10.21",
    "@mastra/core": "^0.16.0",
    "@mastra/memory": "^0.14.4",
    "@mastra/pg": "^0.15.0",
    "@mastra/rag": "^1.2.4",
    "@neondatabase/serverless": "^1.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@t3-oss/env-core": "^0.13.8",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "ai": "^4.3.19",
    "better-auth": "^1.3.7",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "effect": "^3.16.12",
    "hono": "^4.8.8",
    "hono-pino": "^0.10.1",
    "ioredis": "^5.7.0",
    "nanoid": "^5.1.5",
    "pdf-lib": "^1.17.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "posthog-node": "^5.8.1",
    "superjson": "^2.2.2",
    "unpdf": "^1.1.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@types/bun": "^1.2.19",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "nodemon": "^3.1.10",
    "postgres": "^3.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
