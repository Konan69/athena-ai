{
  "name": "@athena-ai/server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently -k -n WEB,MASTRA \"bun run prisma generate && bun --hot run src/server.ts\" \"mastra dev\"",
    "generate": "prisma generate",
    "mastra:build": "mastra build && cp .env .mastra/output/.env",
    "build": "prisma generate && bun build src/server.ts --outdir dist --target bun && bun run mastra:build ",
    "start": "bun run build && mastra start && bun dist/server.js"
  },
  "keywords": [],
